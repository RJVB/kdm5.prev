<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.KSplash">
    <method name="upAndRunning">
      <arg name="component" type="s" direction="in"/>
    </method>
    <method name="setMaxProgress">
      <arg name="percent" type="i" direction="in"/>
    </method>
    <method name="setStartupItemCount">
      <arg name="count" type="i" direction="in"/>
    </method>
    <method name="programStarted">
      <arg name="programIcon" type="s" direction="in"/>
      <arg name="programName" type="s" direction="in"/>
      <arg name="description" type="s" direction="in"/>
    </method>
    <method name="startupComplete" />
    <method name="close" />
  </interface>
</node>
